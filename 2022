CREATE TABLE Enrollment_by_Campus (
    campus_name VARCHAR(255),
    total_enrollment INT,
    retention_rate DECIMAL(5,2),
    campus_type VARCHAR(255)
);

INSERT INTO Enrollment_by_Campus (campus_name, total_enrollment, retention_rate, campus_type) 
VALUES 
