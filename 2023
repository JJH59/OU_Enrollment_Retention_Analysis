-- Gender Distribution Table
CREATE TABLE gender_distribution (
    student_level VARCHAR(50),
    male_count INT,
    female_count INT,
    not_reported_count INT,
    male_percentage DECIMAL(5,2),
    female_percentage DECIMAL(5,2)
);
INSERT INTO gender_distribution (student_level, male_count, female_count, not_reported_count, male_percentage, female_percentage) VALUES
('Undergraduate', 10702, 11339, 5, 48.5, 51.4),
('Graduate', 2743, 3487, 4, 44.0, 55.9),
('Professional', 311, 575, 0, 35.1, 64.9);

