-- Full/Part-Time Enrollment Table
CREATE TABLE full_part_time_enrollment (
    student_level VARCHAR(50),
    enrollment_type VARCHAR(20),
    student_count INT,
    percentage DECIMAL(5,2)
);
INSERT INTO full_part_time_enrollment (student_level, enrollment_type, student_count, percentage) VALUES
