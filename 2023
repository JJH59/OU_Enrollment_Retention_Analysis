-- Enrollment by Campus Table
CREATE TABLE campus_enrollment (
    campus_location VARCHAR(50) PRIMARY KEY,
    total_headcount INT,
    percentage_of_total DECIMAL(5,2)
);
INSERT INTO campus_enrollment (campus_location, total_headcount, percentage_of_total) VALUES
('Norman On-Campus', 28264, 89.4),
('OU-Tulsa - Norman Campus Programs', 485, 1.5),
('Extended Campus', 2884, 9.1);

-- Race/Ethnicity Enrollment Table
CREATE TABLE race_ethnicity_enrollment (
