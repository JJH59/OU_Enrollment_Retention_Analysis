-- Faculty Tenure Status
CREATE TABLE faculty_tenure (
    rank VARCHAR(50),
    tenured INT,
    tenure_track INT,
    not_tenure_track INT,
    not_eligible INT,
    total INT
);

INSERT INTO faculty_tenure (rank, tenured, tenure_track, not_tenure_track, not_eligible, total) VALUES
('Professor', 369, 0, 12, 0, 381),
('Associate Professor', 327, 10, 22, 0, 359),
('Assistant Professor', 0, 211, 56, 3, 270),
('Instructor', 0, 0, 62, 77, 139),
('Lecturer', 0, 0, 62, 57, 119),
('Library Faculty', 14, 0, 0, 0, 14),
('Researchers', 0, 0, 0, 349, 349),
('Deans/Assoc. Deans/Asst. Deans', 40, 0, 3, 0, 43);

-- Graduation Rates
CREATE TABLE graduation_rates (
    rate_type VARCHAR(100),
    cohort_year INT,
    rate DECIMAL(5,2)
);

INSERT INTO graduation_rates (rate_type, cohort_year, rate) VALUES
('1-Year Retention Rate', 2019, 87.1),
('4-Year Graduation Rate', 2016, 53.1),
('6-Year Graduation Rate', 2014, 72.1);
